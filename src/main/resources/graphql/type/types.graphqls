type Tag {
    name: String!
    #    color: Color
}

type Color {
    red: Float!
    green: Float!
    blue: Float!
    alpha: Float
}

enum Visibility {
    PRIVATE
    UNLISTED
    PUBLIC
}

enum Language {
    EN
    RU
}

type Article {
    url: String!
    title: String!
    markdown: String!
    rendered: String!
    markdownPreview: String!
    renderedPreview: String!
    tags: [Tag!]!
    visibility: Visibility!
    language: Language!
    createdAt: Date!
    updatedAt: Date!
}
